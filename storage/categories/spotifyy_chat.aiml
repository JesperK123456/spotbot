    <!--find-->
    <category>
        <pattern>^ WHAT CAN I DO WITH FIND ^</pattern>
        <template><srai>HELP PLAY AND FIND</srai></template>
	</category>

    <category>
        <pattern>^ HELP FIND ^</pattern>
        <template><srai>HELP PLAY AND FIND</srai></template>
	</category>

    <category>
        <pattern>^ FIND *</pattern>
        <template>
            <srai>FIND ITEM <star index="2"/></srai>
        </template>
	</category>

    <category>
        <pattern>^ FIND TRACK *</pattern>
        <template>
            <think><set name="item-type">track</set></think>
            <srai>FIND ITEM <star index="2"/></srai>
        </template>
	</category>

    <category>
        <pattern>^ FIND ALBUM *</pattern>
        <template>
            <think><set name="item-type">album</set></think>
            <srai>FIND ITEM <star index="2"/></srai>
        </template>
	</category>

    <category>
        <pattern>^ FIND ARTIST *</pattern>
        <template>
            <think><set name="item-type">artist</set></think>
            <srai>FIND ITEM <star index="2"/></srai>
        </template>
	</category>

    <category>
        <pattern>^ FIND PLAYLIST *</pattern>
        <template>
            <think><set name="item-type">playlist</set></think>
            <srai>FIND ITEM <star index="2"/></srai>
        </template>
	</category>

